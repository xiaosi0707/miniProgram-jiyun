<!--index.wxml-->
<view class="page">
<swiper indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" class="swiper">
  <block wx:for="{{imgUrls}}" wx:key="idx">
    <swiper-item>
      <image src="{{item}}" class="slide-image"/>
    </swiper-item>
  </block>
</swiper>
<view class="page__bd"></view><!--主体-->
<view class="weui-footer">
  <view class="weui-footer__links">
    <navigator url="" class="weui-footer__link">北京积云教育集团</navigator>
</view>
<view class="weui-footer__text">Copyright © SmallFour</view>
</view><!--未设置页脚-->
</view>
